{
  "proxyServiceUrl": "http://localhost:5000/",
  "permalinkServiceUrl": "http://localhost:5001/",
  "elevationServiceUrl": "http://localhost:5002/",
  "mapInfoService":  "http://localhost:8093/",
  "featureReportService": "http://localhost:5020/",
  "translationsPath": "/translations",
  "assetsPath": "/assets",
  "urlPositionFormat": "extent",
  "urlPositionCrs": "",
  "preserveExtentOnThemeSwitch": true,
  "preserveBackgroundOnThemeSwitch": true,
  "preserveNonThemeLayersOnThemeSwitch": true,
  "allowReorderingLayers": true,
  "allowRemovingThemeLayers": false,
  "searchThemes": true,
  "allowAddingOtherThemes": true,
  "allowFractionalZoom": true,
  "localeAwareNumbers": false,
  "wmsDpi": 96,
  "defaultFeatureStyle": {
    "strokeColor": [0, 0, 255, 1],
    "strokeWidth": 1,
    "strokeDash": [4],
    "fillColor": [255, 0, 255, 0.33],
    "circleRadius": 10,
    "textFill": "black",
    "textStroke": "white"
  },
  "projections": [
    {
      "code": "EPSG:32632",
      "proj": "+proj=utm +zone=32 +datum=WGS84 +units=m +no_defs",
      "label": "WGS 84 / UTM zone 32N"
    },
    {
      "code": "EPSG:2154",
      "proj": "+proj=lcc +lat_1=49 +lat_2=44 +lat_0=46.5 +lon_0=3 +x_0=700000 +y_0=6600000 +ellps=GRS80 +towgs84=0,0,0,0,0,0,0 +units=m +no_defs",
      "label": "RGF93 / Lambert-93"
    }
  ],
  "plugins": {
      "mobile": [
        {
          "name": "Map",
          "mapOptions": {
            "zoomDuration": 250,
            "antialiasing": true
          },
          "toolsOptions": {
            "OverviewSupport": {
              "tipLabel": "Overview"
            },
            "LocateSupport": {
              "keepCurrentZoomLevel": true
            },
            "ScaleBarSupport": {
              "units": "metric"
            }
          }
        },
        {
          "name": "HomeButton"
          ,
          "cfg": {
            "position": 2
          }
        },
        {
          "name": "LocateButton",
          "cfg": {
            "position": 1
          }
        },
        {
          "name": "BackgroundSwitcher",
          "cfg": {
            "position": 0
          }
        },
        {
            "name": "TopBar",
            "cfg": {
              "menuItems": [
                {"key": "ThemeSwitcher", "icon": "themes"},
                {"key": "LayerTree", "icon": "layers", "identifyEnabled": true},
                {"key": "Tools", "icon": "tools", "subitems": [
                  {"key": "IdentifyRegion", "icon": "identify_region"},
                  {"key": "Measure", "icon": "measure"},
                  {"key": "Redlining", "icon": "draw"}
                ]},
                {"key": "Help", "icon": "info", "identifyEnabled": true}
              ],
              "searchOptions": {
                "minScale": 1000,
                "showProviderSelection": true,
                "providerSelectionAllowAll": true,
                "zoomToLayers": false,
                "showProvidersInPlaceholder": false
              },
              "appMenuClearsTask": true,
              "logoClickResetsTheme": true
            }
        },
        {
          "name": "ThemeSwitcher"
        },
        {
          "name": "Measure",
          "cfg": {
            "showMeasureModeSwitcher": true
          }
        },
        {
          "name": "Identify",
          "cfg": {
            "params": {
              "FI_POINT_TOLERANCE": 32,
              "FI_LINE_TOLERANCE": 16,
              "FI_POLYGON_TOLERANCE": 8
            },
            "enableExport": true,
            "longAttributesDisplay": "ellipsis",
            "displayResultTree": true
          }
        },
        {
          "name": "Help"
        },
        {
          "name": "MapCopyright"
        },
        {
          "name": "LayerTree",
          "cfg": {
            "showLegendIcons": true,
            "showRootEntry": false,
            "showQueryableIcon": true,
            "allowMapTips": true,
            "allowCompare": false,
            "allowImport": true,
            "groupTogglesSublayers": false,
            "grayUnchecked": false,
            "flattenGroups": false,
            "layerInfoWindowSize": {"width": 480, "height": 400},
            "bboxDependentLegend": false
          }
        },
        {
          "name": "MapInfoTooltip",
          "cfg": {
            "elevationPrecision": 0,
            "includeWGS84": true
          }
        },
        {
          "name": "IdentifyRegion",
          "cfg": {
            "useWfs": false
          }
        }
      ],
      "desktop": [
        {
          "name": "Map",
          "cfg": {
            "mapOptions": {
              "zoomDuration": 250,
              "antialiasing": true
            },
            "toolsOptions": {
              "OverviewSupport": {
                "tipLabel": "Overview"
              },
              "LocateSupport": {
                "keepCurrentZoomLevel": true
              },
              "ScaleBarSupport": {
                "units": "metric"
              }
            }
          }
        },
        {
          "name": "HomeButton"
          ,
          "cfg": {
            "position": 4
          }
        },
        {
          "name": "LocateButton"
          ,
          "cfg": {
            "position": 3
          }
        },
        {
          "name": "ZoomIn",
          "cfg": {
            "position": 2
          }
        },
        {
          "name": "ZoomOut",
          "cfg": {
            "position": 1
          }
        },
        {
          "name": "BackgroundSwitcher",
          "cfg": {
            "position": 0
          }
        },
        {
            "name": "TopBar",
            "cfg": {
              "menuItems": [
                {"key": "ThemeSwitcher", "icon": "themes"},
                {"key": "LayerTree", "icon": "layers", "identifyEnabled": true},
                {"key": "Tools", "icon": "tools", "subitems": [
                  {"key": "IdentifyRegion", "icon": "identify_region"},
                  {"key": "Measure", "icon": "measure"},
                  {"key": "Redlining", "icon": "draw"}
                ]},
                {"key": "Help", "icon": "info", "identifyEnabled": true}
              ],
              "toolbarItems": [
                {"key": "Measure", "mode": "LineString", "icon": "measure_line"},
                {"key": "Measure", "mode": "Polygon", "icon": "measure_polygon"},
                {"key": "IdentifyRegion", "icon": "identify_region"}
              ],
              "searchOptions": {
                "minScale": 1000,
                "showProviderSelection": true,
                "providerSelectionAllowAll": true,
                "zoomToLayers": false,
                "showProvidersInPlaceholder": false
              },
              "appMenuClearsTask": true,
              "logoUrl": "/"
            }
        },
        {
          "name": "BottomBar",
          "cfg": {
            "viewertitleUrl": "http://qgis.org/",
            "termsUrl":  "http://qgis.org/"
          }
        },
        {
          "name": "Measure",
          "cfg": {
            "showMeasureModeSwitcher": true
          }
        },
        {
          "name": "ThemeSwitcher"
        },
        {
          "name": "LayerTree",
          "cfg": {
            "showLegendIcons": true,
            "showRootEntry": false,
            "showQueryableIcon": true,
            "allowMapTips": true,
            "allowCompare": false,
            "allowImport": true,
            "groupTogglesSublayers": true,
            "grayUnchecked": false,
            "flattenGroups": false,
            "layerInfoWindowSize": {"width": 480, "height": 400},
            "bboxDependentLegend": false
          }
        },
        {
          "name": "Identify",
          "cfg": {
            "params": {
              "FI_POINT_TOLERANCE": 16,
              "FI_LINE_TOLERANCE": 8,
              "FI_POLYGON_TOLERANCE": 4
            },
            "enableExport": true,
            "longAttributesDisplay": "ellipsis",
            "displayResultTree": true,
            "initialWidth": 480,
            "initialHeight": 550
          }
        },
        {
          "name": "MapTip"
        },
        {
          "name": "Help"
        },
        {
          "name": "MapCopyright"
        },
        {
          "name": "Redlining"
        },
        {
          "name": "MapInfoTooltip",
          "cfg": {
            "elevationPrecision": 0,
            "includeWGS84": true
          }
        },
        {
          "name": "IdentifyRegion",
          "cfg": {
            "useWfs": false
          }
        }
      ]
  }
}
